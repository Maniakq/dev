{
  "manifest_version": 2,
  "name": "fireshitone",
  "version": "1.0",
  "description": "Description of your extension",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "16": "icon16-1.png",
      "16": "icon16-2.png",
      "48": "icon48.png",
      "48": "icon48-1.png",
      "48": "icon48-2.png",
      "128": "icon128.png",
      "128": "icon128-1.png",
      "128": "icon128-2.png"
    },
    "default_title": "Your Extension Title",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
